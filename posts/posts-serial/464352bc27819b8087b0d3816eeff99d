V\u000a#include<stdio.h>\u000a\u000a/* Function to get no of set bits in binary\u000a   representation of passed binary no. */\u000aunsigned int countSetBits(int n)\u000a{\u000a    unsigned int count = 0;\u000a    while (n)\u000a    {\u000a      n &= (n-1) ;\u000a      count++;\u000a    }\u000a    return count;\u000a}\u000a\u000a/* Program to test function countSetBits */\u000aint main()\u000a{\u000a    int i = 9;\u000a    printf("%d", countSetBits(i));\u000a    getchar();\u000a    return 0;\u000a}\u000a
